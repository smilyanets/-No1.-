-----------------------------------------------------------------------
package namer;

public class Name{

	public static void main(String[] args) {
		
	int a = 3602;
        int h = a / 3600; a %= 3600;
        
        while (h > 24) h -= 24;
        
        System.out.printf("%d:%02d:%02d", h, a / 60, a % 60);

	}
	
}
