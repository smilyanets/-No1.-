Завдання No8. Сума цифр
-----------------------------------------------------------------------
package namer;

public class Name{

	public static void main(String[] args) {
		int summ = 0;
		int a = 179;
		while (a > 0) {
            summ += a % 10;
            a /= 10;
        }
		System.out.println(summ);
	}

	
}
